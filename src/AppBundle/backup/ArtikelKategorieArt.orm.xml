<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ArtikelKategorieArt" table="ARTIKEL_Kategorie_Art">
    <id name="aAId" type="integer" column="a_a_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="aABeschreibung" type="string" column="a_a_beschreibung" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="aAGoogleMarkerImage" type="string" column="a_a_google_marker_image" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
